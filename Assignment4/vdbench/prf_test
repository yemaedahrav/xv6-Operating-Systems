*SD:	Storage Definitions
sd=sd1,lun=/dev/sdl,threads=16,openflags=o_direct

*WD:	Workload Definitions
wd=WD_0.5K_Write_Random,sd=sd1,rdpct=0,seekpct=100,xfersize=512
wd=WD_0.5K_Write_Sequential,sd=sd1,rdpct=0,seekpct=0,xfersize=512
wd=WD_4K_Write_Random,sd=sd1,rdpct=0,seekpct=100,xfersize=4k
wd=WD_4K_Write_Sequential,sd=sd1,rdpct=0,seekpct=0,xfersize=4k
wd=WD_256K_Write_Random,sd=sd1,rdpct=0,seekpct=100,xfersize=256k
wd=WD_256K_Write_Sequential,sd=sd1,rdpct=0,seekpct=0,xfersize=256k

wd=WD_0.5K_Read_Random,sd=sd1,rdpct=100,seekpct=100,xfersize=512
wd=WD_0.5K_Read_Sequential,sd=sd1,rdpct=100,seekpct=0,xfersize=512
wd=WD_4K_Read_Random,sd=sd1,rdpct=100,seekpct=100,xfersize=4k
wd=WD_4K_Read_Sequential,sd=sd1,rdpct=100,seekpct=0,xfersize=4k
wd=WD_256K_Read_Random,sd=sd1,rdpct=100,seekpct=100,xfersize=256k
wd=WD_256K_Read_Sequential,sd=sd1,rdpct=100,seekpct=0,xfersize=256k

*RD:	Run Definitions
rd=run_0.5K_Write_Random,wd=WD_0.5K_Write_Random,iorate=max,elapsed=60,interval=1,pause=5
rd=run_0.5K_Write_Sequential,wd=WD_0.5K_Write_Sequential,iorate=max,elapsed=60,interval=1,pause=5
rd=run_4K_Write_Random,wd=WD_4K_Write_Random,iorate=max,elapsed=300,interval=1,pause=5
rd=run_4K_Write_Sequential,wd=WD_4K_Write_Sequential,iorate=max,elapsed=60,interval=1,pause=5
rd=run_256K_Write_Random,wd=WD_256K_Write_Random,iorate=max,elapsed=60,interval=1,pause=5
rd=run_256K_Write_Sequential,wd=WD_256K_Write_Sequential,iorate=max,elapsed=60,interval=1,pause=60

